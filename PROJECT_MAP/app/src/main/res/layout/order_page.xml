<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="horizontal">

    <androidx.cardview.widget.CardView
        android:id="@+id/card_view"
        android:layout_width="385dp"
        android:layout_height="165dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:clickable="true"
        android:foreground="?selectableItemBackground"
        card_view:cardBackgroundColor="#ffffff"
        card_view:cardCornerRadius="3dp"
        card_view:cardElevation="3dp"
        card_view:contentPadding="10dp"
        card_view:layout_constraintBottom_toBottomOf="@+id/cardView"
        card_view:layout_constraintEnd_toEndOf="parent"
        card_view:layout_constraintStart_toStartOf="parent"
        card_view:layout_constraintTop_toTopOf="parent"
        card_view:layout_constraintVertical_bias="0.12">

        <RelativeLayout
            android:layout_width="366dp"
            android:layout_height="133dp"
            android:padding="16dp">

            <com.google.android.gms.maps.MapView
                android:id="@+id/mapView"
                android:layout_width="113dp"
                android:layout_height="71dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="237dp"
                android:layout_marginBottom="46dp"
                android:layout_weight="1" />
        </RelativeLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="378dp"
        android:layout_height="96dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        card_view:cardCornerRadius="3dp"
        card_view:cardElevation="3dp"
        card_view:layout_constraintBottom_toBottomOf="parent"
        card_view:layout_constraintTop_toTopOf="parent"
        card_view:layout_constraintVertical_bias="0.656"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="17dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="86dp">

            <TextView
                android:id="@+id/Coupon"
                android:layout_width="70dp"
                android:layout_height="17dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="37dp"
                android:layout_marginTop="27dp"
                android:layout_marginEnd="273dp"
                android:layout_marginBottom="39dp"
                android:text="Coupons" />

            <TextView
                android:id="@+id/Add"
                android:layout_width="80dp"
                android:layout_height="20dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="288dp"
                android:layout_marginTop="27dp"
                android:layout_marginEnd="12dp"
                android:layout_marginBottom="39dp"
                android:text="Add" />

        </RelativeLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView2"
        android:layout_width="381dp"
        android:layout_height="149dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        card_view:cardCornerRadius="3dp"
        card_view:cardElevation="3dp"
        card_view:layout_constraintBottom_toBottomOf="parent"
        card_view:layout_constraintEnd_toEndOf="parent"
        card_view:layout_constraintHorizontal_bias="0.533"
        card_view:layout_constraintStart_toStartOf="parent"
        card_view:layout_constraintTop_toTopOf="parent"
        card_view:layout_constraintVertical_bias="0.426">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="102dp">

            <TextView
                android:id="@+id/Service"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="18dp"
                android:layout_marginTop="9dp"
                android:layout_marginEnd="341dp"
                android:layout_marginBottom="73dp"
                android:text="Service" />

            <TextView
                android:id="@+id/service"
                android:layout_width="104dp"
                android:layout_height="20dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="18dp"
                android:layout_marginTop="36dp"
                android:layout_marginEnd="283dp"
                android:layout_marginBottom="45dp"
                android:text="Laundry Full" />

            <TextView
                android:id="@+id/service2"
                android:layout_width="74dp"
                android:layout_height="21dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="320dp"
                android:layout_marginTop="44dp"
                android:layout_marginEnd="11dp"
                android:layout_marginBottom="36dp"
                android:text="RP.10.000" />

            <TextView
                android:id="@+id/Price"
                android:layout_width="91dp"
                android:layout_height="17dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="305dp"
                android:layout_marginTop="11dp"
                android:layout_marginEnd="9dp"
                android:layout_marginBottom="73dp"
                android:text="Price/Gram" />

        </RelativeLayout>
    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>
